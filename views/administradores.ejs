<%- include('partials/_header') %> <!--PAGINA ADMINISTRADORES DESDE ADMIN-->

<div class="container mt-4">
    <h1 class="mb-3">Lista de Administradores</h1>
    <table class="table table-striped table-bordered table-hover table-dark">

        <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellidos</th>
            <th>Email</th>
        </tr>
        </thead>

        <tbody>
        <% if (admin && admin.length > 0) { %>
            <% admin.forEach(admi => { %>
                <tr>
                    <td><%= admi.name %></td>
                    <td><%= admi.lastName %></td>
                    <td><%= admi.email %></td>
                </tr>
            <% }) %>
        <% } else { %>
            <tr>
                <td colspan="3" class="text-center">No hay profesores para mostrar</td>
            </tr>
        <% } %>
        </tbody>

    </table>
</div>



<%- include('partials/_footer') %>